{
  "name": "ether-goblin",
  "version": "0.1.0",
  "scripts": {
    "compile": "yarn hardhat compile",
    "clean": "yarn hardhat clean && rm -rf ./dist",
    "test": "hardhat test",
    "typecheck": "yarn compile && yarn test && yarn tsc --noEmit",
    "build": "yarn clean && yarn compile && tsc --build && cp -ap ./conf ./dist/",
    "dev-run": "ts-node ./src/main/index.ts",
    "dist-run": "node ./dist/src/main/index.js"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "2.0.5",
    "@nomiclabs/hardhat-etherscan": "3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@tsconfig/node16": "^1.0.2",
    "@typechain/ethers-v5": "9.0.0",
    "@typechain/hardhat": "5.0.0",
    "@types/chai": "4.3.0",
    "@types/chai-as-promised": "7.1.5",
    "@types/mocha": "9.1.0",
    "@types/node": "17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "chai": "4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.10.0",
    "hardhat": "2.9.1",
    "ts-generator": "^0.1.1",
    "ts-node": "10.7.0",
    "typechain": "7.0.1",
    "typescript": "4.6.2"
  },
  "dependencies": {
    "@jovijovi/pedrojs-common": "^1.0.9",
    "@jovijovi/pedrojs-network-http": "^1.0.9",
    "@types/lru-cache": "^7.4.0",
    "ethers": "5.6.0",
    "yaml": "^1.10.2"
  }
}
