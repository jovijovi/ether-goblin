{
  "name": "ether-goblin",
  "version": "0.1.4",
  "description": "A microservice for calling ethereum APIs",
  "author": "jovijovi <mageyul@hotmail.com>",
  "license": "MIT",
  "keywords": [
    "typescript",
    "microservice",
    "ether-goblin",
    "ethereum",
    "ether",
    "goblin",
    "NFT"
  ],
  "scripts": {
    "compile": "yarn hardhat compile",
    "clean": "yarn hardhat clean && rm -rf ./dist",
    "test": "hardhat test",
    "typecheck": "yarn compile && yarn test && yarn tsc --noEmit",
    "build": "yarn clean && yarn compile && tsc --build && cp -ap ./conf ./dist/",
    "dev-run": "ts-node ./src/main/index.ts",
    "dist-run": "node ./dist/src/main/index.js"
  },
  "dependencies": {
    "@jovijovi/pedrojs-common": "^1.0.13",
    "@jovijovi/pedrojs-network-http": "^1.0.13",
    "@jovijovi/pedrojs-version": "^1.0.12",
    "@openzeppelin/contracts": "^4.5.0",
    "@types/lru-cache": "^7.4.0",
    "@types/node-schedule": "^1.3.2",
    "ethers": "5.6.1",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.3"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "2.0.5",
    "@nomiclabs/hardhat-etherscan": "3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@tsconfig/node16": "^1.0.2",
    "@typechain/ethers-v5": "9.0.0",
    "@typechain/hardhat": "5.0.0",
    "@types/chai": "4.3.0",
    "@types/chai-as-promised": "7.1.5",
    "@types/mocha": "9.1.0",
    "@types/node": "17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "chai": "4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.10.0",
    "hardhat": "2.9.1",
    "ts-generator": "^0.1.1",
    "ts-node": "10.7.0",
    "typechain": "7.0.1",
    "typescript": "4.6.2"
  }
}
